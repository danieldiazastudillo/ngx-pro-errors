{
  "name": "ngx-pro-errors",
  "version": "9.0.3",
  "description": "Angular validation module for reactive forms errors. Based and forked from UltimateAngular/ngx-errors",
  "author": {
    "name": "Daniel Diaz Astudillo",
    "url": "https://www.github.com/danieldiazastudillo/ngx-pro-errors"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/danieldiazastudillo/ngx-pro-errors"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-pro-errors-showcase",
    "build": "npm run clean && npm run build:lib && npm run build:demo",
    "clean": "npx rimraf dist",
    "test": "ng test ngx-pro-errors",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy:readme": "copyfiles ./README.md ./projects/ngx-pro-errors",
    "build:lib": "npm run copy:readme && ng build ngx-pro-errors --prod",
    "build:demo": "ng build ngx-pro-errors-showcase --prod --base-href=https://danieldiazastudillo.github.io/ngx-pro-errors/",
    "publish": "npm run publish:demo && npm run publish:lib",
    "publish:demo": "npx angular-cli-ghpages --dir=./dist/ngx-pro-errors-showcase",
    "publish:lib": "cd dist/ngx-pro-errors && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.3",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "bootstrap": "4.5.0",
    "rxjs": "~6.6.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular-devkit/build-ng-packagr": "~0.1000.1",
    "@angular/cli": "~10.0.1",
    "@angular/compiler-cli": "~10.0.3",
    "@angular/language-service": "~10.0.3",
    "@types/jasmine": "3.5.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "14.0.20",
    "angular-cli-ghpages": "0.6.2",
    "codelyzer": "6.0.0",
    "copyfiles": "2.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "rimraf": "3.0.2",
    "ts-node": "8.10.2",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.6"
  }
}
