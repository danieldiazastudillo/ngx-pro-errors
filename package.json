{
  "name": "ngx-pro-errors",
  "version": "10.1.0",
  "description": "Angular validation module for reactive forms errors. Based and forked from UltimateAngular/ngx-errors",
  "author": {
    "name": "Daniel Diaz Astudillo",
    "url": "https://www.github.com/danieldiazastudillo/ngx-pro-errors"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/danieldiazastudillo/ngx-pro-errors"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-pro-errors-showcase",
    "build": "npm run clean && npm run build:lib && npm run build:demo",
    "clean": "npx rimraf dist",
    "test": "ng test ngx-pro-errors",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy:readme": "copyfiles ./README.md ./projects/ngx-pro-errors",
    "build:lib": "npm run copy:readme && ng build ngx-pro-errors --prod",
    "build:demo": "ng build ngx-pro-errors-showcase --prod --base-href=https://danieldiazastudillo.github.io/ngx-pro-errors/",
    "publish": "npm run publish:demo && npm run publish:lib",
    "publish:demo": "npx angular-cli-ghpages --dir=./dist/ngx-pro-errors-showcase",
    "publish:lib": "cd dist/ngx-pro-errors && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "bootstrap": "4.5.3",
    "rxjs": "~6.6.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@angular/language-service": "~10.2.0",
    "@types/jasmine": "3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "14.14.5",
    "angular-cli-ghpages": "0.6.2",
    "codelyzer": "6.0.1",
    "copyfiles": "2.4.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "10.1.2",
    "protractor": "~7.0.0",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "tslib": "2.0.1",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  }
}
